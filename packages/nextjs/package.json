{
  "name": "@frontegg/nextjs",
  "libName": "FronteggNextJs",
  "version": "6.7.14",
  "author": "Frontegg LTD",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently -s all \"yarn build:node\" \"yarn build:stable\" \"yarn build:types\" && yarn build:copy-files",
    "build:watch": "concurrently -s all \"yarn build:node --watch\" \"yarn build:stable --watch\" \"yarn build:types --watch --preserveWatchOutput\"",
    "build:node": "node ../../scripts/build node",
    "build:stable": "node ../../scripts/build stable",
    "build:types": "tsc -b tsconfig.build.json",
    "build:copy-files": "node ../../scripts/copy-files.js"
  },
  "dependencies": {
    "@frontegg/js": "6.69.0",
    "@frontegg/react-hooks": "6.69.0",
    "@types/http-proxy": "^1.17.9",
    "cookie": "^0.5.0",
    "http-proxy": "^1.18.1",
    "iron-session": "^6.3.1",
    "jose": "^4.9.2"
  },
  "devDependencies": {
    "next": ">10.0.0"
  },
  "peerDependencies": {
    "next": ">10.0.0",
    "react": ">16.9.0",
    "react-dom": ">16.9.0"
  }
}
